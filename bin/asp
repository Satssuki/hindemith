#!/usr/bin/env python

__author__ = 'Chick Markley'

import sys
from optparse import OptionParser as OP


def asp(*args):
    """
    front end  for asp commands
    """
    parser = OP()
    parser.add_option("validate")
    print "running asp with args %s" % args

if __name__ == '__main__':
    asp(*sys.argv[1:])
